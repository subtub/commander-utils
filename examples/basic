#!/usr/bin/env node

/**
 * This example shows the smallest application scenario.
 * It will add option --silent and --colorless to the cli.
 */


/**
 * Module dependencies.
 */
var program = require('commander');
var CommanderUtils = require('../');

/**
 * Initialize the commander-utils module.
 */
CommanderUtils(program);

/**
 * Parse the arguments.
 */
program.parseUtils(process.argv);
